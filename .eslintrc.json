{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "search"
        ]
      }
    ]
  }
}
